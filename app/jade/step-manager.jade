.step-manager
  .step-key
    .step-count Step <span id='current-step'>1</span> of <span id='total-steps'>3 : </span>
    .step-title Review And Submit
  .step-wrapper
    .steps
  .ui
    .ui-btn.back 
      img.shadow-icon(data-src="back-arrow")
